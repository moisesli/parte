<h1>Listing descompociciones</h1>

<table>
  <tr>
    <th>Descripcion</th>
    <th>Unidad</th>
    <th>Cantidad</th>
    <th>Terreno</th>
    <th>Cantidadvales</th>
    <th>Observacion</th>
    <th>Parteobra</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @descompociciones.each do |descompocicione| %>
  <tr>
    <td><%= descompocicione.descripcion %></td>
    <td><%= descompocicione.unidad %></td>
    <td><%= descompocicione.cantidad %></td>
    <td><%= descompocicione.terreno %></td>
    <td><%= descompocicione.cantidadvales %></td>
    <td><%= descompocicione.observacion %></td>
    <td><%= descompocicione.parteobra_id %></td>
    <td><%= link_to 'Show', descompocicione %></td>
    <td><%= link_to 'Edit', edit_descompocicione_path(descompocicione) %></td>
    <td><%= link_to 'Destroy', descompocicione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Descompocicione', new_descompocicione_path %>
